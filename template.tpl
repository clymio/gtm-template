const setDefaultConsentState = require('setDefaultConsentState');
const updateConsentState = require('updateConsentState');
const callInWindow = require('callInWindow');
const gtagSet = require('gtagSet');
const injectScript = require('injectScript');
const queryPermission = require('queryPermission');
const log = require('logToConsole');
const setInWindow = require('setInWindow');

const splitInput = (input) => {
  return input.split(',')
      .map(entry => entry.trim())
      .filter(entry => entry.length !== 0);
};

const parseCommandData = (settings) => {
  const regions = splitInput(settings.region);
  const granted = splitInput(settings.granted);
  const denied = splitInput(settings.denied);
  const commandData = {};
  if (regions.length > 0) {
    commandData.region = regions;
  }
  granted.forEach(entry => {
    commandData[entry] = 'granted';
  });
  denied.forEach(entry => {
    commandData[entry] = 'denied';
  });
  return commandData;
};

const main = (data) => {
  gtagSet('ads_data_redaction', data.ads_data_redaction);
  gtagSet('url_passthrough', data.url_passthrough);
  gtagSet('developer_id.dODNhYj', true);
  log(data);
  if (!data.defaultSettings) data.defaultSettings = [{"region":"","granted":"","denied":"ad_storage,ad_user_data,ad_personalization,analytics_storage"}];
  data.defaultSettings.forEach(settings => {
    const defaultData = parseCommandData(settings);
    defaultData.wait_for_update = 500;
    setDefaultConsentState(defaultData);
  });
  if (data.clym_widget_id) {
    
    setInWindow('_clymInit', []);
    callInWindow('_clymInit.push', ['privacy',data.clym_widget_id,{}]);
    
    const clymURL = 'https://widget.clym-sdk.net/clym.js';
    if (queryPermission('inject_script', clymURL)) {
        injectScript(clymURL, data.gtmOnSuccess, data.gtmOnFailure);
       } else {
        data.gtmOnFailure();
         log('Clym script could not be loaded.');
      }
    
    const blockingURL = 'https://widget.clym-sdk.net/blocking.js';
    
    if(data.useAutomaticBlocking){
      if (queryPermission('inject_script', blockingURL)) {
        injectScript(blockingURL, data.gtmOnSuccess, data.gtmOnFailure);
       } else {
        data.gtmOnFailure();
         log('Clym Automatic Blocking could not be loaded.');
      }
    }
  }
};
main(data);
data.gtmOnSuccess();


